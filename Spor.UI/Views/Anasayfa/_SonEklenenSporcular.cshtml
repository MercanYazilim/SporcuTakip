@model IEnumerable<Spor.Core.Entity.Kullanici>
<div class="col-md-4">
    <div class="x_panel">
        <div class="x_title">
            <h2>Son Eklenen Sporcular</h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                    <ul class="dropdown-menu" role="menu">
                        <li>
                            <a href="#">Settings 1</a>
                        </li>
                        <li>
                            <a href="#">Settings 2</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a class="close-link"><i class="fa fa-close"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            @foreach (var item in Model)
    {
        <article class="media event">
            <a class="pull-left date">
                <p class="month">@item.Tarih.ToString("MMMM")</p>
                <p class="day">@item.Tarih.Day</p>
            </a>
            <div class="media-body">
                <a class="title" href="#">@item.AdSoyad</a>
                @*<p>@item.DogumTarihi.ToShortDateString() - @item.Telefon</p>*@
            </div>
        </article>
}
        </div>
    </div>
</div>
